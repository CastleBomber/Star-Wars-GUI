<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="567.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ReadOptionsController">
   <children>
      <VBox prefHeight="567.0" prefWidth="520.0">
         <children>
            <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #22609b;">
               <children>
                  <MenuButton layoutX="18.0" layoutY="62.0" mnemonicParsing="false" text="Entities">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#peopleMenuItemOnAction" text="PEOPLE" />
                      <MenuItem mnemonicParsing="false" onAction="#planetsMenuItemOnAction" text="PLANETS" />
                        <MenuItem mnemonicParsing="false" onAction="#speciesMenuItemOnAction" text="SPECIES" />
                    </items>
                  </MenuButton>
               </children>
            </AnchorPane>
            <TextField fx:id="screen" mouseTransparent="true" prefHeight="372.0" prefWidth="480.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Button fx:id="closeButton" layoutX="450.0" layoutY="527.0" mnemonicParsing="false" onAction="#closeButtonOnAction" text="CLOSE" />
   </children>
</AnchorPane>
