<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="567.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteOptionsController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #22609b;">
         <children>
            <Label layoutX="221.0" layoutY="23.0" text="DELETE" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <MenuButton layoutX="14.0" layoutY="55.0" mnemonicParsing="false" text="Entities">
               <items>
                  <MenuItem mnemonicParsing="false" text="PEOPLE" />
                  <MenuItem mnemonicParsing="false" text="PLANETS" />
                  <MenuItem mnemonicParsing="false" text="SPECIES" />
               </items>
            </MenuButton>
         </children>
      </AnchorPane>
      <Button fx:id="goBackButton" layoutX="20.0" layoutY="526.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" prefHeight="26.0" prefWidth="66.0" text="Go Back" />
      <Button fx:id="closeButton" layoutX="449.0" layoutY="527.0" mnemonicParsing="false" onAction="#closeButtonOnAction" text="CLOSE" />
      <TextField layoutX="224.0" layoutY="228.0" prefHeight="26.0" prefWidth="244.0" />
      <Label layoutX="51.0" layoutY="232.0" text="Primary key of Entity" />
      <Button fx:id="confirmButton" layoutX="86.0" layoutY="457.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="336.0" style="-fx-background-color: #22609b;" text="Confirm" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
