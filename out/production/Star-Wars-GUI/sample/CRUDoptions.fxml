<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="567.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CRUDoptionsController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #22609b;">
         <children>
            <Label layoutX="183.0" layoutY="23.0" text="CRUD Options" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="createButton" layoutX="160.0" layoutY="163.0" mnemonicParsing="false" onAction="#createButtonOnAction" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #0;" text="CREATE">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="readButton" layoutX="160.0" layoutY="242.0" mnemonicParsing="false" onAction="#readButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="READ">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="updateButton" layoutX="160.0" layoutY="308.0" mnemonicParsing="false" onAction="#updateButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="UPDATE">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="deleteButton" layoutX="160.0" layoutY="385.0" mnemonicParsing="false" onAction="#deleteButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="DELETE">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="closeButton" layoutX="440.0" layoutY="517.0" mnemonicParsing="false" onAction="#closeButtonOnAction" text="CLOSE" />
      <Button fx:id="goBackButton" layoutX="37.0" layoutY="505.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" prefHeight="26.0" prefWidth="66.0" text="Go Back" />
   </children>
</AnchorPane>
