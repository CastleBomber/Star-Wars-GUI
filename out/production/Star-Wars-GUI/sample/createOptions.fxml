         <children>
            <Label layoutX="200.0" layoutY="27.0" text="Create Options" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="peopleButton" layoutX="160.0" layoutY="207.0" mnemonicParsing="false" onAction="#peopleButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="PEOPLE" />
      <Button fx:id="starshipsButton" layoutX="160.0" layoutY="271.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="STARSHIPS" />
      <Button fx:id="speciesButton" layoutX="160.0" layoutY="336.0" mnemonicParsing="false" onAction="#speciesButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="SPECIES" />
      <Button fx:id="weaponsButton" layoutX="160.0" layoutY="393.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="WEAPONS" />
